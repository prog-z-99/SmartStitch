[[source]]
url = "https://pypi.org/simple"
verify_ssl = true
name = "pypi"

[scripts]
format = "python -m scripts.formatter"
console = "python -m SmartStitchConsole"
gui = "python -m SmartStitchGUI"
build="pyinstaller Build.spec"
build-no-icon="pyinstaller BuildNoIcon.spec"
mass-stitch = 'python3 SmartStitchConsole.py -sh 10000 -o ${OUTPUT_DIR}  -i ${INPUT_DIR}'

[packages]
natsort = "*"
pillow = "*"
numpy = "*"
pyside6 = "*"
pyqtdarktheme = "*"
psd-tools = "*"

[dev-packages]
flake8 = "*"
black = "*"
isort = "*"
pyinstaller = "*"

[requires]
